<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tech.villageprogrammer.apps.covid19updates">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_corona_4944154"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_corona_4944154"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AffectedCountryListActivity" />
        <activity android:name=".CountryDetailActivity"/>

    </application>

</manifest>